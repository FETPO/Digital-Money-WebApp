{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "npx mix",
        "hot": "npx mix watch --hot",
        "hot-poll": "npx mix watch --hot -- --watch-options-poll=1000",
        "ide": "php artisan ide-helper:generate && php artisan ide-helper:meta && php artisan ide-helper:models -N",
        "intl": "npm run intl-extract && npm run intl-compile && npm run lang-compile",
        "intl-compile": "formatjs compile 'resources/js/messages.json' --format=transifex --out-file=resources/messages/dashboard.json",
        "intl-extract": "formatjs extract 'resources/js/**/*.{js,jsx}' --format=transifex --out-file=resources/js/messages.json --id-interpolation-pattern=[sha512:contenthash:20]",
        "lang-compile": "node compile-lang.js",
        "prepare": "husky install",
        "prod": "npm run production",
        "production": "npx mix --production",
        "watch": "npx mix watch",
        "watch-poll": "npx mix watch -- --watch-options-poll=1000"
    },
    "lint-staged": {
        "resources/**/*.{js,jsx}": [
            "eslint --cache --fix"
        ]
    },
    "browserslist": [
        "> 5%, not dead"
