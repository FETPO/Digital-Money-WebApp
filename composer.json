{
    "type": "project",
    "description": "Cryptitan",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "africastalking/africastalking": "^3.0",
        "akaunting/laravel-money": "dev-cryptitan",
        "aws/aws-sdk-php": "^3.158",
        "barryvdh/laravel-translation-manager": "^0.5.9",
        "beyondcode/laravel-websockets": "^1.12",
        "brick/math": "^0.9.3",
        "doctrine/dbal": "^2.11",
        "egulias/email-validator": "^2.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "geoip2/geoip2": "^2.11",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel-notification-channels/africastalking": "^2.2",
        "laravel-notification-channels/aws-sns": "^1.2",
        "laravel-notification-channels/twilio": "^3.1",
        "laravel/framework": "^8.0",
        "laravel/nexmo-notification-channel": "^2.4",
        "laravel/octane": "^1.0",
        "laravel/sanctum": "^2.8",
        "laravel/telescope": "^4.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "mews/purifier": "^3.3",
        "neoscrypts/bitgo": "*",
        "neoscrypts/cryptitan": "1.x-dev",
        "neoscrypts/server": "1.x-dev",
        "paypal/paypal-checkout-sdk": "^1.0",
        "pda/pheanstalk": "^4.0",
        "pragmarx/google2fa": "^8.0",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.2",
        "pusher/pusher-php-server": "~5.0",
        "spatie/db-dumper": "^3.0",
        "spatie/laravel-csp": "^2.6",
        "spatie/laravel-permission": "^3.18",
        "spatie/laravel-sluggable": "^2.5",
        "spatie/valuestore": "^1.2",
        "stripe/stripe-php": "^7.94",
        "torann/geoip": "^3.0",
        "twilio/sdk": "^6.11",
        "ua-parser/uap-php": "^3.9",
        "wildbit/swiftmailer-postmark": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/neoscrypts/laravel-money"
        },
        {
            "type": "path",
            "url": "./packages/bitgo"
        },
        {
            "type": "composer",
            "url": "https://composer.neoscrypts.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope",
                "fruitcake/laravel-telescope-toolbar"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
